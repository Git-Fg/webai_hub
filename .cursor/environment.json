{
  "version": "1.0",
  "baseImage": "ghcr.io/cursor-images/dart:latest",
  "install": "flutter pub get && npm install",
  "start": "npm run build && flutter pub run build_runner build --delete-conflicting-outputs",
  "env": {
    "FLUTTER_PROJECT_ROOT": "${workspaceFolder}"
  },
  "persistedDirectories": [
    ".dart_tool",
    "build",
    "node_modules"
  ]
}

